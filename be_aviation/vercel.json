{
    "version": 2,
    "builds": [
      {
        "src": "aviation/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "aviation/wsgi.py"
      }
    ],
    "rewrites": [
      { "source": "/static", "destination": "/static" },
      { "source": "/(.*)", "destination": "api/index.py" }
    ],
    "functions": {
      "api/index.py": {
        "runtime": "vercel-python-gis@1.0.0"
      }
    }
  }
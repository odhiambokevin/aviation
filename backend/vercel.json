{
    "version": 2,
    "builds": [
      {
        "src": "yaspi/wsgi.py",
        "use": "git+https://github.com/jperelli/vercel-python-gis.git#main@1.0.0",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "yaspi/wsgi.py"
      }
    ]
  }